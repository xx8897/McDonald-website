AAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,qBAAE,IAAA,CAAA,WAAA,GAAW,CAAI,IAAA,CAAA,WAAjB;AAAA,aAAA;AAGC,WANT,EAIC,SAJD,CAMS,EACT,YAAA,CASS,QATT,EASS;AARP,YAAA,IAAI,EAAC,QAQE;AAPP,qBAAM,wBAOC;AANN,YAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA,EAAA;AAAkB,cAAA,IAAA,CAAA,WAAA,GAAW,KAAX;AAAoC,cAAA,IAAA,CAAA,UAAA,GAAU,IAAV;aAAtD;AAMC,WATT,EAOC,QAPD,CADS,CANT;AAAA,SADe,C;;;;OA3BnB,C,qCA8CQ,CA9CR;AAAA,K;;;;GADF,CAoEI,EAnBJ,YAAA,CAkBa,WAlBb,EAkBa;AAlBD,IAAA,IAAI,EAAC,OAkBJ;AAlBY,IAAA,IAAI,EAAC,QAkBjB;AAlB0B,IAAA,MAAM,EAAN;AAkB1B,GAlBb,E;wBACE;AAAA,aAgBQ,CAhBkB,IAAA,CAAA,U,kBAA1B,YAAA,CAgBQ,gBAhBR,EAgBQ;cAAA;AAhBD,QAAA,KAAK,EAAC;AAgBL,OAhBR,E;AACa,QAAA,OAAO,EAAA,QAAA,CAChB;AAAA,iBAGI,CAHJ,YAAA,CAGI,GAHJ,EAGI,IAHJ,EAGI,C,WAAA,EAFK,YAAA,CACN,MADM,EACN,IADM,EACN,gBAAA,CADe,IAAA,CAAA,oBAAA,EACf,CADM,EAA6B;AAAA;AAA7B,WAEL,E,WAAA,CAHJ,CAGI,CAHJ;AAAA,SADgB,C;AAMP,QAAA,MAAM,EAAA,QAAA,CACf;AAAA,iBAMS,CANT,YAAA,CAMS,QANT,EAMS;AALP,YAAA,IAAI,EAAC,QAKE;AAJP,qBAAM,wBAIC;AAHN,YAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,qBAAE,IAAA,CAAA,gBAAA,IAAA,IAAA,CAAA,gBAAA,OAAA,IAAA,YAAF;AAAA,aAAA;AAGC,WANT,EAIC,MAJD,CAMS,CANT;AAAA,SADe,C;;;;OAPnB,C,qCAgBQ,CAhBR;AAAA,K;;;;GADF,CAmBI,CAvIN,C","sourcesContent":["<template>\r\n  <div class=\"cart\">\r\n    <h6 class=\"cart__title\" v-if=\"cartList.length < 1\">購物車是空的！</h6>\r\n    <transition-group\r\n      tag=\"ul\"\r\n      class=\"cart__list\"\r\n      name=\"slideDown\"\r\n      mode=\"out-in\"\r\n    >\r\n      <li class=\"cart__list__item\" v-for=\"item in cartList\" :key=\"item.id\">\r\n        <h6 class=\"cart__list__item__title\">\r\n          <div\r\n            class=\"cart__list__item__cover\"\r\n            :style=\"{ backgroundImage: 'url(' + item.pic + ')' }\"\r\n          ></div>\r\n          <span>{{ item.title }}</span>\r\n        </h6>\r\n        <div class=\"cart__list__item__counter\">\r\n          <button\r\n            type=\"button\"\r\n            class=\"button button--round\"\r\n            :disabled=\"item.qty < 2\"\r\n            @click=\"setQty(false, item)\"\r\n          >\r\n            －\r\n          </button>\r\n          <input\r\n            type=\"number\"\r\n            min=\"1\"\r\n            name=\"qty\"\r\n            :value=\"item.qty\"\r\n            @change=\"updateQty(item, $event)\"\r\n          />\r\n          <button\r\n            type=\"button\"\r\n            class=\"button button--round\"\r\n            @click=\"setQty(true, item)\"\r\n          >\r\n            ＋\r\n          </button>\r\n        </div>\r\n        <div class=\"cart__list__item__price\">\r\n          <button\r\n            type=\"button\"\r\n            class=\"button button--refuse\"\r\n            @click=\"removeItemFormCart(item)\"\r\n          >\r\n            移除\r\n          </button>\r\n          <span\r\n            >小計：{{ (item.qty * item.price).toLocaleString(\"zh-TW\")\r\n            }}<small> 元</small></span\r\n          >\r\n        </div>\r\n      </li>\r\n    </transition-group>\r\n    <div class=\"cart__price\">\r\n      <span>總計：</span>\r\n      <span>{{ total.toLocaleString(\"zh-TW\") }}<small> 元</small></span>\r\n      <button\r\n        type=\"button\"\r\n        class=\"button button--checkout button--confirm\"\r\n        @click=\"showConfirm = !showConfirm\"\r\n        :disabled=\"cartList.length < 1\"\r\n      >\r\n        送出訂單\r\n      </button>\r\n    </div>\r\n    <transition name=\"scale\" mode=\"out-in\" appear>\r\n      <Modal title=\"確認訂單\" v-if=\"showConfirm\">\r\n        <template #content>\r\n          <h6 class=\"modal__title\" style=\"text-align: left\">餐點</h6>\r\n          <ul class=\"cart__list modal__list\">\r\n            <li\r\n              class=\"cart__list__item\"\r\n              v-for=\"item in cartList\"\r\n              :key=\"item.id\"\r\n            >\r\n              <div\r\n                class=\"cart__list__item__cover\"\r\n                :style=\"{ backgroundImage: 'url(' + item.pic + ')' }\"\r\n              ></div>\r\n              <h6 class=\"cart__list__item__title\">\r\n                {{ item.title }} x {{ item.qty }}\r\n              </h6>\r\n              <span class=\"modal__list__price\">\r\n                小計：{{ (item.qty * item.price).toLocaleString(\"zh-TW\")\r\n                }}<small> 元</small>\r\n              </span>\r\n            </li>\r\n          </ul>\r\n          <div class=\"modal__list__total\">\r\n            <span>訂單金額：</span>\r\n            <span>{{ total.toLocaleString(\"zh-TW\") }}<small> 元</small></span>\r\n          </div>\r\n        </template>\r\n        <template #action>\r\n          <button\r\n            type=\"button\"\r\n            class=\"button button--refuse\"\r\n            @click=\"showConfirm = !showConfirm\"\r\n          >\r\n            回上一步驟\r\n          </button>\r\n          <button\r\n            type=\"button\"\r\n            class=\"button button--confirm\"\r\n            @click=\"\r\n              showConfirm = false;\r\n              showResult = true;\r\n            \"\r\n          >\r\n            訂單完成\r\n          </button>\r\n        </template>\r\n      </Modal>\r\n    </transition>\r\n    <transition name=\"scale\" mode=\"out-in\" appear>\r\n      <Modal title=\"訂購成功\" v-if=\"showResult\">\r\n        <template #content>\r\n          <p>\r\n            您的取餐號碼為<span>{{ generateRandomNumber() }}</span\r\n            >，請至櫃台等號付款。\r\n          </p>\r\n        </template>\r\n        <template #action>\r\n          <button\r\n            type=\"button\"\r\n            class=\"button button--confirm\"\r\n            @click=\"closeResultModal\"\r\n          >\r\n            關閉\r\n          </button>\r\n        </template>\r\n      </Modal>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { defineComponent } from \"vue\";\r\nimport Modal from \"../components/Modal.vue\";\r\n\r\nexport default defineComponent({\r\n  name: \"Cart\",\r\n  components: { Modal },\r\n  data() {\r\n    return {\r\n      showConfirm: false,\r\n      showResult: false,\r\n    };\r\n  },\r\n  computed: {\r\n    cartList(): {\r\n      id: string;\r\n      kind: string;\r\n      title: string;\r\n      pic: string;\r\n      price: number;\r\n      qty: number;\r\n    }[] {\r\n      return this.$store.getters[\"sortedCartList\"];\r\n    },\r\n    total() {\r\n      if (this.cartList.length < 1) {\r\n        return 0;\r\n      } else {\r\n        const reducer = (accumulator: any, currentValue: any) =>\r\n          accumulator + currentValue;\r\n\r\n        const total: number = this.cartList\r\n          .map((item: { price: number; qty: number }) => {\r\n            return item.qty * item.price;\r\n          })\r\n          .reduce(reducer);\r\n\r\n        return total;\r\n      }\r\n    },\r\n  },\r\n  methods: {\r\n    setQty(\r\n      plus: boolean,\r\n      updateTarget: {\r\n        id: string;\r\n        kind: string;\r\n        title: string;\r\n        pic: string;\r\n        price: number;\r\n        qty: number;\r\n      }\r\n    ) {\r\n      if (plus) {\r\n        updateTarget.qty += 1;\r\n        return this.$store.dispatch(\"updateCart\", updateTarget);\r\n      } else {\r\n        updateTarget.qty -= 1;\r\n        return this.$store.dispatch(\"updateCart\", updateTarget);\r\n      }\r\n    },\r\n    updateQty(\r\n      updateTarget: {\r\n        id: string;\r\n        kind: string;\r\n        title: string;\r\n        pic: string;\r\n        price: number;\r\n        qty: number;\r\n      },\r\n      e: any\r\n    ) {\r\n      updateTarget.qty = Number(e.target.value);\r\n      return this.$store.dispatch(\"updateCart\", updateTarget);\r\n    },\r\n    removeItemFormCart(removeTarget: { id: string }) {\r\n      return this.$store.dispatch(\"removeItemFormCart\", removeTarget.id);\r\n    },\r\n    generateRandomNumber(): string {\r\n      let result = \"\";\r\n      let n = 0;\r\n\r\n      for (let index = 0; index < 4; index++) {\r\n        n = Math.floor(Math.random() * 9);\r\n\r\n        if (result.indexOf(String(n)) !== -1) {\r\n          index -= 1;\r\n          continue;\r\n        } else {\r\n          result += n;\r\n        }\r\n      }\r\n\r\n      return result;\r\n    },\r\n    closeResultModal() {\r\n      this.showResult = false;\r\n      return this.$store.dispatch(\"clearCart\");\r\n    },\r\n  },\r\n});\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.cart {\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 20%;\r\n  height: 100%;\r\n  background-color: $color-white;\r\n  border-radius: $border-radius;\r\n\r\n  @include breakpoint-xl {\r\n    width: 40%;\r\n  }\r\n\r\n  @include breakpoint-md {\r\n    width: 50%;\r\n  }\r\n\r\n  &__title {\r\n    margin: 0;\r\n    padding: $grid-gap $grid-gap 0;\r\n    color: $color-gray-thin;\r\n    font-size: $font-size-black;\r\n    font-weight: $font-weight-bold;\r\n    text-align: center;\r\n  }\r\n\r\n  &__list {\r\n    @include list-style;\r\n    max-height: calc(100% - 100px);\r\n    padding: $grid-gap $grid-gap 0;\r\n    overflow-y: auto;\r\n\r\n    &__item {\r\n      display: flex;\r\n      align-items: center;\r\n      flex-wrap: wrap;\r\n      margin-top: calc(#{$grid-gap}/ 2);\r\n\r\n      &:first-child {\r\n        margin-top: 0;\r\n      }\r\n\r\n      &__cover {\r\n        width: 40px;\r\n        height: 40px;\r\n        margin-right: 0.5rem;\r\n        border-radius: $border-radius-round;\r\n        background-size: cover;\r\n        background-position: center;\r\n        overflow: hidden;\r\n      }\r\n\r\n      &__title {\r\n        display: flex;\r\n        align-items: center;\r\n        margin: 0.5rem 0;\r\n      }\r\n\r\n      &__counter {\r\n        margin-left: auto;\r\n      }\r\n\r\n      &__price {\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        flex-wrap: wrap;\r\n        width: 100%;\r\n        color: $color-gray;\r\n        font-size: $font-size-light;\r\n\r\n        .button {\r\n          margin: 0.5rem 0;\r\n        }\r\n\r\n        span {\r\n          margin-left: auto;\r\n        }\r\n      }\r\n    }\r\n  }\r\n\r\n  &__price {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    flex-wrap: wrap;\r\n    margin-top: auto;\r\n    padding: $grid-gap;\r\n\r\n    span {\r\n      &:last-of-type {\r\n        font-size: $font-size-bold;\r\n        font-weight: $font-weight-bold;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}